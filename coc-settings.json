{
  "coc.preferences.diagnostic.displayByAle": true,
  "phpls.enable": true,
  "python.linting.enabled": false,
  "python.sortImports.args": ["-rc --multi-line=3 --trailing-comma"],
  "python.sortImports.path": "isort",
  "go.enable": true,
  "html.enable": true,
  "html.format.enable": false,
  "html.filetypes": ["html", "htm", "handlebars"],
  "css.filetypes": ["css", "less", "wxss", "scss"],
  "css.validate": true,
  "less.validate": true,
  "wxss.validate": true,
  "scss.validate": true,
  "tsserver.enable": true,
  "tsserver.trace.server": "verbose",
  "tsserver.log": "verbose",
  "vetur.format.enable": false,
  "yaml.schemas": {
    "url": "*.yaml",
    "Kubernetes": "*.yaml"
  },
  "yaml.validate": true,
  "yaml.completion": true,
  "json.enable": true,
  "json.format.enable": false,
  "docker.enable": true,
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": ["-c", "~/.vim_runtime/efm-langserver-config.yaml"],
      "filetypes": ["vim", "eruby", "markdown"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "css.enable": true
}
